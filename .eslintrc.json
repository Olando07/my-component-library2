{
    "extends": ["react-app", "react-app/jest", "prettier"],
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": "error",
        "no-console": "warn",
        "no-unused-vars": "off",
        "@typescript-eslint/no-unused-vars": "off",
        "testing-library/no-node-access": "off",
        "react-hooks/rules-of-hooks": "off"
    }
}
